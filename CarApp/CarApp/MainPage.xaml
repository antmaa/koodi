        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="712,356,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" RenderTransformOrigin="0.028,0.344" />
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="516,356,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top"/>

    </Grid>
